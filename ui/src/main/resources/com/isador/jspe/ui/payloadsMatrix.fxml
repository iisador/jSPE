<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>
<TitledPane xmlns:fx="http://javafx.com/fxml/1" animated="false" prefHeight="600.0" prefWidth="800.0" text="Payloads"
            xmlns="http://javafx.com/javafx/16" fx:controller="com.isador.jspe.ui.PayloadsController">
    <VBox>
        <TableView id="payloads">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <ButtonBar>
            <buttons>
                <Button onMouseClicked="#addPayload" text="+"/>
                <Button id="btnRemovePayload" onMouseClicked="#removePayload" text="-" disable="true"/>
            </buttons>
        </ButtonBar>
    </VBox>
</TitledPane>
